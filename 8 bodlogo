#7
def dawhardsan(a):
    return list(dict.fromkeys(a))
mylist = dawhardsan(['1234', 'qwer', 567, 567, 'qwer'])
print(mylist)




#8
def maxim(a, b, c):
    list = [a, b, c]
    return max(list)
a = 4
b = 7
c = 8
print(maxim(a, b, c))
