        s=s+1
    if (i>='A' and i<'Z'):
        x=x+1        
print('Tom useg: ',x)
print('Jijig useg: ',s)




#3 
def urjver(a):
    b = 1
    for i in a:
        b = b * i
    return b


numbers=[3,4,5,2,1]
print(urjver(numbers))




#4
def func(x):
    s = 1
    for i in range(2, x + 1):
        s *= i
    return s
num=int(input())
print(func(num))




#5
def rev(n):
    list2.reverse()
    return n
list2 = [5, 6, 8, 12, 4, 11]
print(rev(list2))




