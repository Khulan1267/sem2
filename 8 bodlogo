#1
def palindrome(n):
    x=n
    r=0
    while(n>0):
        dig=n%10
        r=r*10+dig
        n=n//10
    if(x==r):
        print("Palindrome too")
    else:
        print("Palindrome bish")
n=int(input())
palindrome(n)



